# Variables d'environnement pour TURNKEY

# Google Gemini API Key (OBLIGATOIRE)
GOOGLE_GEMINI_API_KEY=your_gemini_api_key_here

# Admin (optionnel - par défaut : admin/admin123)
ADMIN_DEFAULT_USERNAME=admin
ADMIN_DEFAULT_PASSWORD=admin123

# Configuration système (optionnel - valeurs par défaut OK)
MAX_UPLOAD_FILES=5
MAX_FILE_SIZE_MB=2
SESSION_RETENTION_HOURS=24
